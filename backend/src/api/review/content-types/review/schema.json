{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "rating": {
      "type": "decimal",
      "max": 10,
      "min": 0,
      "required": true
    },
    "album": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::album.album",
      "inversedBy": "review"
    }
  }
}
